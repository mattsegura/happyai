{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "seed": "tsx scripts/seedDatabase.ts",
    "seed-fixed": "tsx scripts/seedDatabaseFixed.ts",
    "clear-db": "tsx scripts/clearDatabase.ts",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "perf:benchmark": "tsx scripts/performance-benchmark.ts",
    "perf:load-test": "k6 run scripts/load-test.js",
    "perf:load-test:small": "k6 run --vus 100 --duration 2m scripts/load-test.js",
    "perf:load-test:medium": "k6 run --vus 500 --duration 5m scripts/load-test.js",
    "perf:load-test:large": "k6 run --vus 1000 --duration 10m scripts/load-test.js",
    "perf:stress-test": "k6 run --stage 1m:500 --stage 2m:1000 --stage 2m:2000 --stage 1m:0 scripts/load-test.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.22.0",
    "@splinetool/react-spline": "^4.1.0",
    "@stripe/stripe-js": "^8.2.0",
    "@supabase/supabase-js": "^2.75.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.344.0",
    "motion": "^12.23.24",
    "openai": "^6.7.0",
    "preline": "^3.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0",
    "stripe": "^19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^5.1.0",
    "globals": "^15.9.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.7",
    "workbox-window": "^7.3.0"
  }
}
